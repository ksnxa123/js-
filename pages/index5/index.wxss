.contanier{
  display: flex;
  align-items: center;
  margin: 30rpx 30rpx 0 30rpx;
  padding-bottom: 10rpx;
  border-bottom: 4rpx solid #E6E6E6;
}
.wrapper-title{
  flex: 1 1 auto;
  display: flex;
  flex-direction: column;
  height: 180rpx;
}
.wrapper-title .text{
  flex: 2;
  margin-bottom: 40rpx;
  font-size: 32rpx;
  font-weight: 500;
  line-height: 1.5;
  overflow: hidden;
  text-overflow: ellipsis;
  display:-webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-word;
  white-space: pre-line;
}
.wrapper-title .wrapper-icon{
  flex: 1;
  display: flex;
  width: 100%;
  align-items: center;
}
.contanier-icon{
  flex-basis: 140rpx;
  font-size: 28rpx;
  color: #A4A4A4;
}
.img{
  flex: 0 0 auto;
  width: 220rpx;
  height: 160rpx;
  margin-left: 40rpx;
  border-radius: 8rpx;
}